#include<stdio.h>
#include<string.h>
int main()
{
    char a[100];
    int i,j=0;
    scanf("%s",a);
    for(i=0;i<strlen(a);i++)
    {
        if(a[0]!=a[i])
            {
                j=1;
                break;
            }
    }
    if(j==0)
        printf("Yes");
    else
        printf("No");
}
